    "@priyang/react-component-lib": "^0.2.3",
    "@reduxjs/toolkit": "^1.8.1",
    "date-fns": "^2.28.0",
    "framer-motion": "^6.2.8",
    "idb": "^7.0.2",
    "invert-color": "^2.0.0",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-cookie": "^4.1.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-draggable": "^4.4.5",
    "react-dropzone": "^14.2.2",
    "react-icons": "^4.4.0",
    "react-page-visibility": "^7.0.0",
    "react-player": "^2.10.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "react-share": "^4.4.1",
    "react-toastify": "^9.0.7",
    "redux-persist": "^6.0.0",
    "typescript": "^4.6.3",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "resolutions": {
    "redux-persist/@types/react": "17.0.43",
    "redux-persist/@types/react-dom": "17.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:test": "DISABLE_ESLINT_PLUGIN=true env-cmd -f .env.test react-scripts start",
    "msw": "npx msw init ./public",
    "msw:cleanup": "rm public/mockServiceWorker.js",
    "prebuild:ci": "yarn msw:cleanup",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "build:remove": "rm -rf build",
    "test": "react-scripts test --watchAll=false  --forceExit",
    "test:debug": "DEBUG_PRINT_LIMIT=10000 react-scripts test",
    "coverage": "react-scripts test --watchAll --coverage",
    "testAll": "react-scripts test --watchAll",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "Page": "sh Page.sh",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .tsx,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.4.0",
    "@types/jsmediatags": "^3.9.3",
    "@types/node": "^17.0.8",
    "@types/react": "^18.0.20",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^18.0.6",
    "@types/react-page-visibility": "^6.4.1",
    "@types/react-redux": "^7.1.22",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-window-infinite-loader": "^1.0.6",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "history": "^5.3.0",
    "msw": "^0.47.4",
    "prettier": "^2.6.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
